<link rel="stylesheet" href="/src/public/css/capacitacion/capacitacion.css">
<form action="/usuario/listar/{{user.id_usuario}}" method="get"></form>
<div id="container">
    <div class="row">
        {{#each lista}}
        <div id="tarjeta">
            <div id="id_capacitacion">
                <p>{{nombres}}</p>
            </div>
            <div id="codigo">
                <p>{{descripcion}}</p>
            </div>
            <div  id="boton">
            <div id="editar"> </div>
            <a href="/usuario/editar"{{id_capacitacion}}></a> <button type="button">Editar</button>
           <div id="eliminar"> </div>
           <a href="/usuario/eliminar"{{id_capacitacion}}></a> <button type="button">Eliminar</button>
         </div> 
        </div>
        {{#each }}
    </div>
</div>